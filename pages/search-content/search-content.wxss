/* pages/search-content/search-content.wxss */

@import "../template/blog-item/blog-item-template.wxss"
@import "../template/bloger-item/bloger-item-template.wxss"
.container {
  display: flex;
  flex-direction: column;
}

.search-wrap {
  display: flex;
  flex-direction: row;
  padding-bottom: 10rpx;
  position: fixed;
  top: 0rpx;
  background-color: #fff;
  width: 100%;
}

/*字体图标*/

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.iconsousuo:before {
  content: "\e613";
}

.search-icon {
  color: #999;
  font-size: 24rpx;
  width: 50rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  background-color: #f4f4f4;
  margin-left: 50rpx;
  border-top-left-radius: 14px;
  border-bottom-left-radius: 14px;
}

.search-input {
  width: 540rpx;
  height: 60rpx;
  background-color: #f4f4f4;
}

.place-search {
  color: #999;
  font-size: 30rpx;
}

.delete-search {
  background-color: #f4f4f4;
  border-top-right-radius: 14px;
  border-bottom-right-radius: 14px;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}

.iconguanbi:before {
  content: "\e658";
}

.delete-icon {
  font-size: 24rpx;
  color: #333;
}

.search-pos {
  height: 120rpx;
}

.search-tag {
  display: flex;
  flex-direction: row;
  height: 40rpx;
  line-height: 40rpx;
  position: fixed;
  top: 70rpx;
  width: 100%;
  text-align: center;
  padding-bottom: 10rpx;
  background-color: #fff;
}

.search-tag-left {
  background-color: #f4f4f4;
  margin-left: 50rpx;
  flex: 1;
}

.search-tag-right {
  background-color: #f4f4f4;
  flex: 1;
  margin-right: 50rpx;
}
