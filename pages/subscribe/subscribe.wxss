/* pages/subscribe/subscribe.wxss */

@import "../template/blog-item/blog-item-template.wxss"
.subscribe {
  display: flex;
  flex-direction: column;
}

.subscribe-nav {
  display: flex;
  flex-direction: row;
  /*background-color: rgb(0, 144, 240);*/
  background-color: #b3d4db;
  height: 100rpx;
  justify-content: space-between;
}

.subscribe-nav-title {
  line-height: 100rpx;
  color: #fff;
  font-size: 30rpx;
  font-weight: 600;
  margin-left: 20rpx;
}

.subscribe-nav-msg {
  line-height: 100rpx;
  margin-right: 20rpx;
}

/*字体图标*/

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.iconweidu:before {
  content: "\e641";
}

.subscribe-nav-icon {
  color: #fff;
  font-size: 40rpx;
}

.subscribe-nav-msg-num {
  color: #fff;
  font-size: 24rpx;
}
